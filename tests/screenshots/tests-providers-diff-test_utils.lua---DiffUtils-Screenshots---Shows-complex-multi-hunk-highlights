09|          a * b                                                                 
10|      }                                                                         
11|▌                                                                               
12|▌     /// Divides two numbers with error handling                               
13|▌     fn divide(a: i32, b: i32) -> Result<i32, &'static str> {                  
14|▌         if b == 0 {                                                           
15|▌             Err("Division by zero")                                           
16|▌         } else {                                                              
17|▌             Ok(a / b)                                                         
18|▌         }                                                                     
19|▌     }                                                                         
20|  }                                                                             
