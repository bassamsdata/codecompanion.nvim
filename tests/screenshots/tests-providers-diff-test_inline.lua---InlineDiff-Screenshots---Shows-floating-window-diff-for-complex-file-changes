02|  │  class UserManager {                                                      │ 
03|  │    constructor() {                                                        │ 
04|  │      this.users = [];                                                     │ 
05|  │▌     this.nextId = 1;                                                     │ 
06|  │    }                                                                      │ 
07|  │                                                                           │ 
08|  │    addUser(name, email) {                                                 │ 
09|  │      this.users.push({ name, email });                                    │ 
10|  │▌     if (!name || !email) {                                               │ 
11|  │▌       throw new Error('Name and email are required');                    │ 
12|  │▌     }                                                                    │ 
13|  │▌     const user = { id: this.nextId++, name, email, active: true };       │ 
14|  │▌     this.users.push(user);                                               │ 
15|  │▌     return user;                                                         │ 
16|  │    }                                                                      │ 
17|  │                                                                           │ 
18|  │    getUser(name) {                                                        │ 
19|  │      return this.users.find(u => u.name === name);                        │ 
20|  │▌   getUser(identifier) {                                                  │ 
21|  │▌     if (typeof identifier === 'number') {                                │ 
22|  │▌       return this.users.find(u => u.id === identifier);                  │ 
23|  │▌     }                                                                    │ 
